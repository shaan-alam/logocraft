generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DB_URI")
  directUrl = env("DB_URI_DIRECT")
}

model Logo {
  id        String   @id @default(uuid())
  name      String
  logoURLs  String[]
  userId    String
  createdAt DateTime @default(now())
  isPublic  Boolean  @default(false)
}
